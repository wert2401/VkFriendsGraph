<Window x:Class="VkFriendsGraph.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VkFriendsGraph"
        mc:Ignorable="d"
        Title="Friends Graph" Height="450" Width="800"
        WindowStartupLocation="CenterScreen" WindowState="Maximized"
        KeyDown="Window_KeyDown"
        MouseWheel="Window_MouseWheel"
        >
    <Grid>
        <Frame NavigationUIVisibility="Hidden" x:Name="Frame" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,0,0,0"/>
    </Grid>
</Window>
